{
  "name": "wholepage-slider",
  "version": "1.0.0",
  "description": "You can see a working [demo here](https://elansx.github.io/Wholepage-Slider/)",
  "main": "dist/slider.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "test": "cross-env node_modules/.bin/jest --verbose",
    "serve": "cross-env node_modules/.bin/parcel serve src/index.html",
    "build": "cross-env node_modules/.bin/parcel build src/slider.ts",
    "push": "cross-env rm -rf dist/ && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elansx/Wholepage-Slider.git"
  },
  "author": "elansx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elansx/Wholepage-Slider/issues"
  },
  "homepage": "https://github.com/elansx/Wholepage-Slider#readme",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "babel-jest": "^26.0.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.8",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  }
}
